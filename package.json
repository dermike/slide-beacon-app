{
  "name": "electron-slide-beacon",
  "version": "0.1.2",
  "description": "Electron app to broadcast URLs as an Eddystone Physical Web beacon",
  "main": "main.js",
  "scripts": {
    "package": "node ./node_modules/electron-packager/cli.js ./ EddystoneURL --platform=darwin --arch=x64 --version=0.34.0 --ignore=node_modules/electron-rebuild --asar --icon=icon.icns",
    "electron": "node ./node_modules/electron-prebuilt/cli.js ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dermike/electron-slide-beacon.git"
  },
  "keywords": [
    "electron"
  ],
  "author": "Mikael Jergefelt",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "5.1.1",
    "electron-prebuilt": "0.34.0",
    "electron-rebuild": "1.0.2"
  },
  "dependencies": {
    "eddystone-beacon": "1.0.3",
    "ws": "0.8.0"
  }
}
